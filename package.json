    "@types/lodash": "^4.14.97",
    "@types/mocha": "^2.2.47",
    "@types/moment": "^2.13.0",
    "@types/node": "^9.4.0",
    "@types/power-assert": "^1.4.29",
    "@types/raven": "^2.1.4",
