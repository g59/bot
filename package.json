{
  "name": "slack-channel-active-reminder",
  "description": "remind non acctive channel at slack",
  "version": "1.0.0",
  "author": "github.com/9renpoto",
  "bugs": {
    "url": "https://github.com/9renpoto/slack-channel-active-reminder/issues"
  },
  "dependencies": {
    "@slack/client": "5.0.1",
    "moment": "2.24.0"
  },
  "devDependencies": {
    "@9renpoto/eslint-config": "4.7.0",
    "cross-env": "5.2.0",
    "husky": "2.4.1",
    "lint-staged": "8.2.1",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "5.0.0",
    "rimraf": "2.6.3",
    "typedoc": "0.14.2",
    "typescript": "3.5.2",
    "typescript-eslint-parser": "21.0.2"
  },
  "engines": {
    "node": ">=7.10"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/9renpoto/slack-channel-active-reminder#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "slack"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "prettier-eslint --write",
      "eslint --ext=ts",
      "npm run build:docs",
      "git add"
    ],
    "*.{md,json,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/slack-channel-active-reminder.git"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:docs": "typedoc --out docs",
    "build:ts": "tsc",
    "clean": "rimraf lib",
    "lint": "run-p lint:*",
    "lint:es": "eslint . --cache --ext=ts",
    "prepare": "run-s clean build",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "start": "node lib/index.js",
    "test": "run-p test:*",
    "test:build": "npm run lint",
    "test:lint": "npm run lint"
  },
  "types": "lib/index.d.ts"
}
